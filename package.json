{
	"name": "@insertcoinab/gwen-react-charts",
	"version": "1.0.1",
	"private": false,
	"license": "SEE LICENSE IN LICENSE",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/*",
		"LICENSE"
	],
	"scripts": {
		"dev": "vite",
		"build": "rm -rf dist && yarn build:ts && yarn build:types",
		"build:ts": "babel src/chart --extensions '.ts,.tsx' --out-dir dist --copy-files --ignore spec.ts",
		"build:types": "tsc --project ./tsconfig-build.json --emitDeclarationOnly --skipLibCheck"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.5",
		"@babel/core": "^7.22.1",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/preset-env": "^7.22.4",
		"@babel/preset-react": "^7.22.3",
		"@babel/preset-typescript": "^7.21.5",
		"@types/react": "^18.2.7",
		"@types/react-dom": "^18.2.4",
		"babel-loader": "^9.1.2",
		"typescript": "^5.0.4",
		"vite": "^4.4.12",
		"web-vitals": "^3.4.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"dependencies": {
		"@nivo/core": "^0.83.0",
		"@nivo/sankey": "^0.83.0"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react",
			"@babel/preset-typescript"
		],
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		]
	}
}
